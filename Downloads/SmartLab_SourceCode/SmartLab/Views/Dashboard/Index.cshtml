    @section Scripts{
        <script type="text/javascript">



              document.querySelector('#infrabtn').addEventListener('click', function () {

                     var url='@Url.Action("Dashboard_Options", "Dashboard", null, null)';
                     window.open(url, '_blank');
        }, false);
        document.querySelector('#vkmbtn').addEventListener('click', function () {

            var url = '@Url.Action("Index", "BudgetingOrder", new { isDashboard = "Plan Order Summary" }, null)';
            window.open(url, '_blank');
        }, false);

            document.querySelector('#hwbtn').addEventListener('click', function () {
                debugger;
                     var url='@Url.Action("Index", "HWDamage",null, null)';
                window.open(url,'_blank');
            }, false);

            document.querySelector('#tsuabtn').addEventListener('click', function () {
                debugger;
                     var url='@Url.Action("Index", "TSUA",null, null)';
                window.open(url,'_blank');
                }, false);

        </script>
        <script src="~/Scripts/Chart_UI.js"></script>
        <script src="../../Scripts/core.js" type="text/javascript"></script>
