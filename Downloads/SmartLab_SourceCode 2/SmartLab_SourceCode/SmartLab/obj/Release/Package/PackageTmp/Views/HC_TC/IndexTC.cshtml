    @section Scripts{

        <script type="text/javascript">


            $(document).ready(function () {
                debugger;
                $.ajax({

                    type: "Get",
                    async: false,
                    url: "/HC_TC/Lookup_TC",
                    success: onsuccess_lookupdata,
                    error: onerror_lookupdata
                })


                function onsuccess_lookupdata(data) {
                    debugger;
                    var code_list = JSON.parse(data.data.Data.Content);

                    var year_list = [];
                    var currentYear = (new Date()).getFullYear();
                    for (var i = 2021; i <= currentYear + 1; i++) {
                        year_list.push({ "Year": i });

                    }
                    debugger;
                    GenerateTC(code_list, year_list);


                }

                function onerror_lookupdata(response) {
                    alert("Error lookup");

                }




            });







        </script>

    }
