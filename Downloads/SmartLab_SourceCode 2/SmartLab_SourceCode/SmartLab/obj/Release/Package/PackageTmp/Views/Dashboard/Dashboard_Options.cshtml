                document.getElementById("selectLabId").options[1].disabled = true;
            }
            else {
                var myDdl = document.getElementById("selectLabId");
                var myOption1 = document.createElement("OPTION");
                myOption1.innerText = "----------------------------------------------------------------------------------";
                myDdl.options.add(myOption1, index1);
                document.getElementById("selectLabId").options[index1].disabled = true;
            }






