        // function for loading labtypes into dropdown
        function loadLabTypes() {
            debugger;
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                url: "../CPCWW/GetLabTypes",
                dataType: 'json',
                success: function (data) {
                    //debugger;
                    //alert(data);
                    for (var i = 0; i < data.data.length; i++) {
                        //debugger;
                        $("#selectLabType").append(
                            $('<option/>', {
                                value: data.data[i].Value,
                                html: data.data[i].Text
                            }));
                    }
                    $(".selectpicker").selectpicker('refresh');
                }
            });
        }

        // function for loading locations into dropdown
        function loadLocations() {
            debugger;
            document.getElementById('selectLocation').options.length = 0;
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                url: "../CPCWW/GetLocation",
                //data: { LabTypes: LabTypes },
                dataType: 'json',
                success: function (data) {
                    debugger;
                    //alert(data);
                    for (var i = 0; i < data.data.length; i++) {
                        // debugger;
                        $("#selectLocation").append(
                            $('<option/>', {
                                value: data.data[i].Value,
                                html: data.data[i].Text
                            }));
                    }

                    $('.selectpicker').selectpicker('refresh');
                    debugger;
                    document.getElementById("gbt1").disabled = false;
                    document.getElementById("gbt1").style.pointerEvents = 'auto';
                }
            });
        }
        // when labtype is changed
        function fnOnchangeLabType() {
            debugger;
            // if labtype dropdown does not contains a selected value
            if (document.getElementById('selectLabType').selectedIndex > -1) {
                // disable and empty Location dropdown
                document.getElementById('selectLocation').disabled = false;

                // refresh dropdowns
                $('.selectpicker').selectpicker('refresh');
                //function call for loading locations into dropdown
                loadLocations();


            }

            else {
                //if Location dropdown has no values selected
                document.getElementById('selectLocation').options.length = 0;
                document.getElementById('selectLocation').disabled = true;

                $('.selectpicker').selectpicker('refresh');
                // disable submit button until both the dropdowns has value
                document.getElementById("gbt1").disabled = true;
                document.getElementById("gbt1").style.pointerEvents = 'none';
            }





        }

        //function for loading OEMs
        function loadOEMs() {
            debugger;
            // ajax call to get the data
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                url: "../CPCWW/GetOEMs",
                dataType: 'json',
                success: function (data) {
                    //debugger;
                    //alert(data);
                    for (var i = 0; i < data.data.length; i++) {
                        //debugger;
                        //populate values in dropdown
                        $("#selectOEM").append(
                            $('<option/>', {
                                value: data.data[i].Value,
                                html: data.data[i].Text
                            }));
                    }
                    $(".selectpicker").selectpicker('refresh');

                    document.getElementById("gbt3").disabled = false;
                    document.getElementById("gbt3").style.pointerEvents = 'auto';
                }
            });
        }

    </script>

