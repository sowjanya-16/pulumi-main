            const items = document.querySelectorAll(".accordion a");
            function toggleAccordion() {

                this.classList.toggle('active');
                this.nextElementSibling.classList.toggle('active');
            }
            items.forEach(item => item.addEventListener('click', toggleAccordion));



            function fnOnchangeLabType(lctype_selected) {
                debugger;
                $("#selectSite").val('').trigger('change');
                if (document.getElementById('selectLabType').selectedIndex > -1) {

                    document.getElementById('selectSite').disabled = false;
                    var lctypeOptions = lctype_selected && lctype_selected.options;
                    $('.selectpicker').selectpicker('refresh');
                    //loadLocations();
                }

                else {
                    debugger;

                    document.getElementById('selectLabId').options.length = 0;
                    document.getElementById('selectSite').disabled = true;
                    document.getElementById('selectLabId').disabled = true;
                    $('.selectpicker').selectpicker('refresh');
                }



                if (document.getElementById('selectSite').selectedIndex > -1) {

                    document.getElementById('selectLabId').disabled = false;
                    //document.getElementById('selectLabId').disabled = false;

                    $('.selectpicker').selectpicker('refresh');
                }

                else {
                    document.getElementById('selectLabId').disabled = true;
                    $('.selectpicker').selectpicker('refresh');
                }

            }


            function fnSitesChange(siteSelect) {
                debugger;
                fnfetchSiteChosen();
                $('#selectLocation').change(function () {
                    $('#selectLabId option:first').prop('selected', 'selected');

                });
                var siteOptions = siteSelect && siteSelect.options;

                if (siteOptions.selectedIndex > -1) {
                    $("#selectLabId").prop('disabled', false);

                    $.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "../Diagnostics/GetLabIDs",
                        data: JSON.stringify({ LabTypes: $('#selectLabType').val(), Locations: $('#selectSite').val() }),
                        dataType: 'json',
                        //traditional: true,
                        success: function (data) {
                            debugger;
                            document.getElementById('selectLabId').options.length = 0;
                            if (data.data.length > 0) {
                                for (var i = 0; i < data.data.length; i++) {
                                    //debugger;
                                    $("#selectLabId").append(
                                        $('<option/>', {
                                            value: data.data[i].Value,
                                            html: data.data[i].Text
                                        }));
                                }
                                $("#selectLabId").html($("#selectLabId option").sort(function (a, b) {
                                    debugger;
                                    return a.text == b.text ? 0 : a.text < b.text ? -1 : 1
                                }))
                               // $("#selectLabId").get(0).selectedIndex = 0;
                            }
                            else {

                                $("#selectLabId").append(
                                    $('<option/>', {
                                        value: "No HILS for this filter",
                                        html: "No HILS for this filter"
                                    }));
                            }

                            //$.notify('LabIDs are successfully fetched.', {
                            //    globalPosition: "top center",
                            //    className: "success"
                            //});
                            debugger;

                            $('.selectpicker').selectpicker('refresh');


                        }
                    });

                }
                else {
                    $("#selectLabId").prop('disabled', true);
                }
                var siteSel;
                document.getElementById("selectLabId").innerHTML = "";
                var labOptions = "";
                @*for (var j = 0, jLen = siteOptions.length; j < jLen; j++) {
                    debugger;
                        siteSel = siteOptions[j];


