        /*buuton hover & clik effets*/
        .glow-on-hover {
            /* width: 220px;
      height: 50px;*/
            border: none;
            outline: none;
            background: white;
            color: #111;
            cursor: pointer;
            position: relative;
            z-index: 0;
            border-radius: 10px;
            display: none;
        }

            .glow-on-hover:before {
                content: '';
                background: linear-gradient(45deg, #408977, #4682B4, #408977, #4682B4, #408977, #4682B4,#408977, #4682B4);
                position: absolute;
                top: -2px;
                left: -2px;
                background-size: 400%;
                z-index: -1;
                filter: blur(5px);
                width: calc(100% + 4px);
                height: calc(100% + 4px);
                /*animation: glowing 20s linear infinite;*/
                opacity: 0;
                transition: opacity .3s ease-in-out;
                border-radius: 10px;
                bottom: 0px;
                position: absolute;
                right: 0px;
            }

            .glow-on-hover:active {
                /*color: #000*/
            }

                .glow-on-hover:active:after {
                    background: transparent;
                }

            .glow-on-hover:hover:before {
                opacity: 1;
            }

            .glow-on-hover:after {
                z-index: -1;
                content: '';
                position: absolute;
                width: 100%;
                height: 100%;
                background: #f0f0f0;
                border-radius: 5px;
                bottom: 0px;
                position: absolute;
                right: 0px;
            }




