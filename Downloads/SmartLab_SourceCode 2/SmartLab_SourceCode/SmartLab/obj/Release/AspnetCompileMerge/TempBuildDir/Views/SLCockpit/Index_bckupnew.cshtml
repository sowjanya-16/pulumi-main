                //Function to check the validity of user entered Dates

                function validateDate() {
                    debugger;
                    // regular expression to match required date format
                    re = /^\d{1,2}\/\d{1,2}\/\d{4}$/;
                    var startDate = document.getElementById('startDate').value;
                    var endDate = document.getElementById('endDate').value;
                    var _sDate = Date.parse(startDate);
                    var _eDate = Date.parse(endDate);


                    if (document.getElementById('startDate').value == '' && document.getElementById('endDate').value == '') {
                        alert("Dates not selected ");
                        document.getElementById('startDate').focus();
                        return false;
                    }

                    else if (document.getElementById('startDate').value == '') {
                        alert("Start Date not selected ");
                        document.getElementById('startDate').focus();
                        return false;
                    }

                    else if (document.getElementById('endDate').value == '') {
                        alert("End Date not selected ");
                        document.getElementById('endDate').focus();
                        return false;
                    }

                    else if (document.getElementById('startDate').value != '' && !document.getElementById('startDate').value.match(re)) {
                        alert("Invalid date format: " + document.getElementById('startDate').value);
                        document.getElementById('startDate').focus();
                        return false;
                    }

                    else if (document.getElementById('endDate').value != '' && !document.getElementById('endDate').value.match(re)) {
                        alert("Invalid Date format: " + document.getElementById('endDate').value);
                        document.getElementById('endDate').focus();
                        return false;
                    }

                    else if (_sDate > _eDate) {
                        alert("Inappropriate Selection of Dates");
                        document.getElementById('startDate').focus();
                    }

                    else
                        return true;

                }

