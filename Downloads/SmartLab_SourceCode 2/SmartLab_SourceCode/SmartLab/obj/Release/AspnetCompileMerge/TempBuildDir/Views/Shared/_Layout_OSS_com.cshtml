<!DOCTYPE html>
<html>
<head>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-GKX0NP1SYR"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-GKX0NP1SYR');
    </script>

    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <script src="~/Scripts/umd/popper.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script src="~/Scripts/notify.min.js"></script>

    <style type="text/css">
        .container-fluid body-content {
            position: absolute;
            min-height: 800px;
            min-width: 800px;
            overflow: hidden;
        }


        #content {
            position: absolute;
        }

            #content img {
                position: absolute;
                top: 10px;
                right: 10px;
            }
    </style>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - SmartLab Portal</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/DevExtremeBundle")
    @Scripts.Render("~/Scripts/DevExtremeBundle")
</head>
<body style="position:absolute;width: 100vw;">
    <div>
        <img style="width:100%;" src="~/Content/images/Bosch-Supergraphic.jpg" />
    </div>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="#">
            <img src="~/Content/images/cockpit.PNG" style="height:30px; width:30px;" class="d-inline-block align-top" alt="">
            SmartLab
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <text id="nav_home" class="nav-link" style="cursor:pointer">Home <span class="sr-only">(current)</span></text>
                </li>
                <li class="nav-item">
                    <text id="nav_contact" class="nav-link" style="cursor:pointer">Contact</text>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Weekly Average Utilization
                    </a>
                    <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <a href="@Url.Action("WeeklyAverage", "CPC" )" ,
                           target="_blank" id="nav_graph" class="nav-link" title="This link will redirect weekly average View." style="cursor:pointer">
                            Weekly Average Utilization (Loc)
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="@Url.Action("WeeklyAveragePrj", "CPC" )" ,
                           target="_blank" id="nav_graph" class="nav-link" title="This link will redirect weekly average View." style="cursor:pointer">
                            Weekly Average Utilization (Project)
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="@Url.Action("WeeklyAverageCharts", "CPC" )" ,
                           target="_blank" id="nav_graph" class="nav-link" title="This link will redirect weekly average View." style="cursor:pointer">
                            Weekly Average Charts
                        </a>




                    </div>
                </li>
            </ul>

        </div>
        <div class="mr-sm-2" id="divUserName" style="height:24px; ">@User.Identity.Name</div>
        <img class="my-2 my-sm-0" style="width:110px; height:24px; " src="~/Content/images/bosch.jpeg" />
    </nav>


    <div class="container-fluid body-content">
        @RenderBody()
        <hr />
        <footer id="footer" class="navbar-text" style="width:100%">
            <div style="width:100%; font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif; font-size:small">
                <span>V1.0.0.3 - Developed by NE2-VS SmartLab Software Team @DateTime.Now.Year - Robert Bosch Engineering and Business Solutions Ltd</span>
                <span style="float:right">-- Best viewed in Mozilla / MS Edge --</span>
            </div>

        </footer>
    </div>


    @*<script src="~/Scripts/jquery-3.5.1.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/jquery.validate.min.js"></script>
        <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
        <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
        <script src="~/Scripts/notify.min.js"></script>*@
    <script>
        $(document).ready(function () {
            $('#nav_home').click(function () {
                var url='@Url.Action("Index", "CPC")';
                window.location.href=url;
            });
            $('#nav_contact').click(function () {
                var url='@Url.Action("Contact","Home")';
                window.location.href=url;
            });
        });

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
