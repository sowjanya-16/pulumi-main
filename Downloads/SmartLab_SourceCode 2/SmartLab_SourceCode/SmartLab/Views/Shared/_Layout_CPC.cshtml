<script>
        $(document).ready(function () {
            $('#nav_home').click(function () {
                var url='@Url.Action("Index", "CPC")';
                window.location.href=url;
            });
            $('#nav_contact').click(function () {
                var url='@Url.Action("Contact","Home")';
                window.location.href=url;
            });

            @*$('#nav_graph').click(function () {
                var url='@Html.Action("Lab_Report", "LabCar", new { reload=false} )';
                window.location.href=url;
            });*@


            var modal = document.getElementById("myModal");

            // Get the button that opens the modal
            var btn = document.getElementById("nav_def");

            // Get the <span> element that closes the modal
            var span = document.getElementsByClassName("close")[0];

            // When the user clicks the button, open the modal
            btn.onclick = function () {
                modal.style.display = "block";

                debugger;
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "/CPC/GetTimeConfig",
                    datatype: "json",
                    success: function (data) {
                        debugger;
                        for (var row = 0; row <= 1; row++) {
                            for (var col = 0; col < 6; col++) {
                                if (row == 1 && col == 1) {
                                    $("#TimeConfig").children().children()[row].children[col].innerHTML = data.data[0].Night_From_Time;
                                }
                                if (row == 1 && col == 2) {
                                    $("#TimeConfig").children().children()[row].children[col].innerHTML = data.data[0].Night_To_Time;
                                }
                                if (row == 1 && col == 3) {
                                    $("#TimeConfig").children().children()[row].children[col].innerHTML = data.data[0].Weekend_From_Time;
                                }
                                if (row == 1 && col == 4) {
                                    $("#TimeConfig").children().children()[row].children[col].innerHTML = data.data[0].Weekend_To_Time;
                                }
                                if (row == 1 && col == 5) {
                                    $("#TimeConfig").children().children()[row].children[col].innerHTML = data.data[0].Date_Modified;
                                }
                            }
                        }
                    },
                });
    
            }

            // When the user clicks on <span> (x), close the modal
            span.onclick = function () {
                modal.style.display = "none";
            }

            // When the user clicks anywhere outside of the modal, close it
            window.onclick = function (event) {
                if (event.target == modal) {
                    modal.style.display = "none";
                }
            }

           
        });

</script>
